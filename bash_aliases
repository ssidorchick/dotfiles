# git
alias gst='git status'
alias gpl='git pull'
alias gco='git checkout'
alias gad='git add'
alias gci='git commit'
alias gps='git push'
alias gro='git ro'
alias gcl='git cleanup'
alias giff='git diff'

alias gci.='git add .; git commit'

alias gsl='git slist'
alias gsa='git ssave'
alias gsh='git sshow'
alias gsp='git sapply'
alias gspo='git spop'
alias gsd='git sdrop'


# ls
alias la='ls -a'
